{
    "resource_group_name": "${RESOURCE_GROUP_NAME}",
    "location": "${AZURE_LOCATION}",
    "use_billing_policy": "${USE_BILLING_POLICY:-false}",
    "azd_environment_name": "${AZURE_ENV_NAME}",
    "resource_share_user": ${RESOURCE_SHARE_USER},
    "deploy_github_runner": "${DEPLOY_GITHUB_RUNNER:-false}",
    "enable_failover_github_runner": "${ENABLE_FAILOVER_GITHUB_RUNNER:-false}",
    "power_platform_environment": {
        "id": "${POWER_PLATFORM_ENVIRONMENT_ID}",
        "name": "${POWER_PLATFORM_ENVIRONMENT_NAME:-Copilot Studio + Azure AI}",
        "language_code": "${POWER_PLATFORM_ENVIRONMENT_LANGUAGE_CODE:-1033}",
        "security_group_id": "${POWER_PLATFORM_ENVIRONMENT_SECURITY_GROUP_ID:-00000000-0000-0000-0000-000000000000}",
        "currency_code": "${POWER_PLATFORM_ENVIRONMENT_CURRENCY_CODE:-USD}",
        "environment_type": "${POWER_PLATFORM_ENVIRONMENT_TYPE:-Sandbox}",
        "location": "${POWER_PLATFORM_ENVIRONMENT_LOCATION}"
    },
    "github_runner_config": {
        "image_name": "${GITHUB_RUNNER_IMAGE_NAME:-github-runner}",
        "image_tag": "${GITHUB_RUNNER_IMAGE_TAG:-latest}",
        "github_pat": "${GITHUB_PAT}",
        "github_repo_owner": "${GITHUB_REPO_OWNER}",
        "github_repo_name": "${GITHUB_REPO_NAME}",
        "github_runner_group": "${GITHUB_RUNNER_GROUP:-default}",
        "github_runner_image_branch": "${GITHUB_RUNNER_IMAGE_BRANCH:-main}",
        "min_replicas": 0,
        "max_replicas": 1,
        "cpu_requests": "1.0",
        "memory_requests": "2Gi",
        "workload_profile_type": "${WORKLOAD_PROFILE_TYPE:-D4}"
    },
    "networking": {
        "primary_virtual_network": {
            "id": "${PRIMARY_VNET_ID}",
            "primary_subnet_id": "${PRIMARY_SUBNET_ID}",
            "pe_primary_subnet_id": "${PE_PRIMARY_SUBNET_ID}",
            "github_runner_primary_subnet_id": "${GITHUB_RUNNER_PRIMARY_SUBNET_ID}",
            "deployment_script_container_subnet_id": "${DEPLOYMENT_SCRIPT_CONTAINER_SUBNET_ID}"
        },
        "failover_virtual_network": {
            "id": "${FAILOVER_VNET_ID}",
            "failover_subnet_id": "${FAILOVER_SUBNET_ID}",
            "pe_failover_subnet_id": "${PE_FAILOVER_SUBNET_ID}",
            "github_runner_failover_subnet_id": "${GITHUB_RUNNER_FAILOVER_SUBNET_ID}"
        }
    }
}
